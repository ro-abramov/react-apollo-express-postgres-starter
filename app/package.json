{
  "name": "app-starter",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/api": {
      "target": "http://localhost:3100/"
    },
    "/subscriptions": {
      "target": "ws://localhost:3100",
      "ws": true
    }
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.1.9",
    "apollo-client": "2.2.5",
    "apollo-link": "1.2.1",
    "apollo-link-http": "1.5.3",
    "apollo-link-ws": "1.0.7",
    "apollo-server-express": "1.3.2",
    "apollo-utilities": "1.0.9",
    "babel-loader": "7.1.4",
    "babel-plugin-emotion": "9.0.1",
    "body-parser": "1.18.2",
    "cross-env": "5.1.3",
    "emotion": "9.0.2",
    "emotion-theming": "9.0.0",
    "express": "4.16.2",
    "graphql": "0.13.1",
    "graphql-subscriptions": "0.5.8",
    "graphql-tag": "2.8.0",
    "graphql-tools": "2.21.0",
    "pg": "7.4.1",
    "react": "16.2.0",
    "react-apollo": "2.0.4",
    "react-dom": "16.2.0",
    "react-emotion": "9.0.2",
    "react-scripts": "1.1.1",
    "sequelize": "4.35.2",
    "subscriptions-transport-ws": "0.9.6"
  },
  "scripts": {
    "develop:client": "react-app-rewired start",
    "develop:server": "npm-run-all migrate server:develop",
    "develop": "cross-env NODE_ENV=development npm-run-all --parallel develop:*",
    "prebuild": "rimraf ./build",
    "build": "react-app-rewired build",
    "server:start": "node ./src/server/index.js",
    "server:pm2": "pm2-runtime process.yml",
    "server:develop": "nodemon ./src/server/index.js",
    "start": "cross-env NODE_ENV=production npm-run-all build migrate server:start",
    "production": "cross-env NODE_ENV=production npm-run-all build migrate server:pm2",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "migrate": "sequelize db:migrate",
    "sequelize": "sequelize"
  },
  "devDependencies": {
    "dotenv": "5.0.1",
    "morgan": "1.9.0",
    "nodemon": "1.17.1",
    "npm-run-all": "4.1.2",
    "react-app-rewired": "1.5.0",
    "rimraf": "2.6.2",
    "sequelize-cli": "4.0.0"
  }
}
